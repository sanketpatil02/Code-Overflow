int main()
{
    int a[5];
    
    printf("Enter Array Elements \n");
    
    for(int i=0;i<5;i++) {
        printf("Enter a[%d]=",i);
        scanf("%d",&a[i]);
    }
    
    int max = a[0];
    
    for(int i=0;i<5;i++) {
        if(a[i]>max) {
            max = a[i];
        }
    }
    printf("The max of Array is = %d",max);
}
