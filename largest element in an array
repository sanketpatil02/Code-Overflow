#include <stdio.h>

int main() {
   int array[10] = {1, 2, 3, 4, 5, 6, 7, 8, 9, 0};
   int i, largest;

   largest = array[0];
   
   for(i = 1; i< 10; i++) {
      if( largest < array[i] ) 
         largest = array[i];
   }
   
   printf("Largest element of array is %d", largest);   
   
   return 0;
}
